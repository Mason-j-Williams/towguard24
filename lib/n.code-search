   32:     const newLocation: UserLocation = {
   33        lat: pos.coords.latitude,
   34:       lng: pos.coords.longitude,
   35        accuracy: pos.coords.accuracy,
   36:       heading: pos.coords.heading,
   37        speed: pos.coords.speed,

   40  
   41:     // Filter Jitter: Only update if accuracy improves significantly or we moved significantly (>5m)
   42:     setLocation(prev => {
   43        if (!prev) {
   44:         localStorage.setItem(STORAGE_KEY, JSON.stringify(newLocation));
   45:         return newLocation;
   46        }
   47  
   48:       // Simple haversine-lite distance check
   49:       const dLat = (newLocation.lat - prev.lat) * (Math.PI / 180);
   50:       const dLon = (newLocation.lng - prev.lng) * (Math.PI / 180);
   51:       const a = Math.sin(dLat / 2) * Math.sin(dLat / 2) +
   52:                 Math.cos(prev.lat * (Math.PI / 180)) * Math.cos(newLocation.lat * (Math.PI / 180)) *
   53:                 Math.sin(dLon / 2) * Math.sin(dLon / 2);
   54:       const c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));
   55:       const distMeters = 6371e3 * c;
   56  
   57:       // Only update state if movement is significant (>5m) or accuracy improved by >20%
   58:       if (distMeters > 5 || newLocation.accuracy < (prev.accuracy * 0.8)) {
